{
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/old-path",
      "status": 301,
      "headers": { "Location": "/new-path" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
